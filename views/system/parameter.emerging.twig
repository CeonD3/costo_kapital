{% extends "system/main.twig" %}

{% block css %}
<style>
	.wTableValue {
		width: 110px;
	}
</style>
{% endblock %}

{% block content %}
	{% set data = rsp.data %}
	<section class="slide-content">
		<div class="slide-reflex">
			<div class="container container-custom">
				<div class="row">
					{% include "system/information.twig" %}
				</div>
				<div class="row panel-parameter-market" id="AppParameterEmerging">
					<div class="col-lg-12">
						<div class="card-panel">
							<div class="row">
								<div class="col-lg-12 mb-4">
									<div class="bg-five text-center py-2">
										<h4 class="m-0">Costo de Capital Sectorial de un Mercado Emergente (D칩lares)</h4>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-lg-4 col-md-6 float-left">
									<div class="">
										<label class="label-control">13. Par치metros del Costo de Capital Financiero </label>
										<div class="table-responsive">
											<table class="table table-item">
												<tbody>
													{% for item in data.system.finance.parameters %}
													<tr>
														<td class="bgone">{{ item.label }}</td>
														<td class="wTableValue">{{ item.value }}</td>
													</tr>
													{% endfor %}
												</tbody>
											</table>
										</div>
										<div class="table-responsive">
											<table class="table table-item">
												<tbody>
													{% for item in data.system.finance.results %}
													<tr>
														<td class="bgtwo">{{ item.label }}</td>
														<td class="wTableValue">{{ item.value }}</td>
													</tr>
													{% endfor %}
												</tbody>
											</table>
										</div>
									</div>
								</div>
								<div class="col-lg-4 col-md-6 float-left">
									<div class="">
										<label class="label-control">14. Par치metros del Costo de Capital Econ칩mico </label>
										<div class="table-responsive">
											<table class="table table-item">
												<tbody>
													{% for item in data.system.economic.parameters %}
													<tr>
														<td class="bgone">{{ item.label }}</td>
														<td class="wTableValue">{{ item.value }}</td>
													</tr>
													{% endfor %}
												</tbody>
											</table>
										</div>
										<div class="table-responsive">
											<table class="table table-item">
												<tbody>
													{% for item in data.system.economic.results %}
													<tr>
														<td class="bgtwo">{{ item.label }}</td>
														<td class="wTableValue">{{ item.value }}</td>
													</tr>
													{% endfor %}
												</tbody>
											</table>
										</div>
									</div>
								</div>
								<div class="col-lg-4 col-md-6 float-left">
									<div class="">
										<label class="label-control">15. Costo de la deuda (Kd) </label>
										<div class="table-responsive">
											<table class="table table-item">
												<tbody>
													{% for item in data.system.debt.parameters %}
													<tr>
														<td class="bgone">{{ item.label }}</td>
														<td class="wTableValue">{{ item.value }}</td>
													</tr>
													{% endfor %}
												</tbody>
											</table>
										</div>
										<div class="table-responsive">
											<table class="table table-item">
												<tbody>
													{% for item in data.system.debt.results %}
													<tr>
														<td class="bgtwo">{{ item.label }}</td>
														<td class="wTableValue">{{ item.value }}</td>
													</tr>
													{% endfor %}
												</tbody>
											</table>
										</div>
									</div>
								</div>
							</div>
							<div class="row">
								<div class="col-lg-9 float-left mx-auto">
									<div id="groupChart" style="min-height:300px;"></div>
								</div>
							</div>
							<div class="row panel-navigation">
								<div class="col-lg-12 text-center">
									<a href="/estructura-mercado-desarrollado/{{ rsp.data.code }}" class="float-left" style="font-weight:600;">
										<i class="fa fa-home-o mt-2" style="font-size:20px;" aria-hidden="true">Ir a Mercado Desarrollado</i>
									</a>
									<a href="/documentos/{{ rsp.data.code }}" style="font-weight:600;">
										<i class="fas fa-home-o mt-2" style="font-size:20px;"> Ir a Reportes</i>
									</a>
									<a href="/promedio-mercado-emergente/{{ rsp.data.code }}" class="float-right">
										<i class="fa fa-arrow-right" aria-hidden="true"></i>
									</a>
								</div>
							</div>
							{# {% include 'system/navigation.twig' with  { _back: '/estructura-mercado-emergente/' ~ rsp.data.code, _next: '/promedio-mercado-emergente/' ~ rsp.data.code, _code: rsp.data.code } %} #}
						</div>
					</div>
					{% include 'system/publicidad.twig' %}
				</div>
			</div>
		</div>
	</section>
{% endblock %}

{% block js %} 
	<script type="text/javascript">
		let args = JSON.parse("{{ rsp.data |json_encode|e('js') }}");
		window.addEventListener('load', window.AppParameterEmerging.init(args));
	</script>
{% endblock %}